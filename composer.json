{
    "name": "zenky/zenky",
    "description": "Zenky PHP SDK.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "tzurbaev",
            "email": "zurbaev@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "^7.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "mockery/mockery": "^1.5",
        "friendsofphp/php-cs-fixer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Zenky\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zenky\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "fix": "php-cs-fixer fix",
        "check-codestyle": "php-cs-fixer fix --dry-run --diff --diff-format udiff --verbose"
    }
}
